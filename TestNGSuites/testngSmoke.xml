<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	
  <groups>
  <run>
   <include name="Smoke"></include>
  </run>
  </groups>

  <listeners>
   <listener class-name="common_test_utilities.Listener"></listener>
  </listeners>

  <test thread-count="1" name="End to End framework test" parallel="false">
   <classes>
   <class name="android_tests.GeneralStoreApp"/>
   </classes>
  </test>
  
  <test thread-count="1" name="Error Message Test" parallel="false">
   <classes>
   <class name="android_tests.ErrorMessage_GeneralStoreApp"/>
   </classes>
  </test>
  
</suite>
